   <style>
        /* Google Fonts for Arabic Typography */

        :root {
            --brand-color: #b08045; /* The golden brown color from the image */
            --white: #ffffff;
        }

     
        /* Main Card Layout */
        .card {
      display: inline-flex;
    color: var(--white);
    overflow: hidden;
    border-radius: 4px;
        }

        /* Left Section: Logo and Brand */
        .left-section {
            flex: 1;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            padding: 40px;
            position: relative;
        }

        /* Vertical Divider Line */
        .left-section::after {
            content: "";
            position: absolute;
            right: 0;
            top: 15%;
            bottom: 15%;
            width: 1.5px;
            background-color: rgba(255, 255, 255, 0.4);
        }

        .logo-box {
            margin-bottom: 20px;
            width: 180px;
        }

        .brand-name {
          font-size: 26px;
    font-weight: 700;
    letter-spacing: 4px;
    margin: 0;
    text-align: center;
    line-height: 1;
        }

        .brand-sub {
          font-size: 18px;
   
     font-weight: 700;
        }

        /* Right Section: Contact and QR */
    .right-section {
    flex: 1.2;
    display: block;
    padding: 50px;
    text-align: center;
    padding-top: 67px;
}
        /* Top part of right section: QR Code */
        .qr-area {
            display: flex;
            flex-direction: column;
            align-items: center;
            align-self: flex-end;
            margin-bottom: 20px;
        }
.contact-item a {
    letter-spacing: 1px;
    margin-left: 2px;
    
}
.contact-item {
 display: flex;
    align-items: center;
    gap: 8px;
    font-size: 22px;
    text-decoration: none;
    color: white;
    margin-top: 8px;

}
        .qr-code {
            background: white;
            padding: 10px;
            border-radius: 8px;
            width: 120px;
            height: 120px;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .qr-code img {
            width: 100%;
            height: auto;
        }

        .website-url {
          font-size: 13px;
    color: white;
    text-decoration: none;
    letter-spacing: 0.5px;
        }

        /* Bottom part of right section: Contact details */
        .contact-details {
            display: flex;
            flex-direction: column;
            align-items: flex-start;
            gap: 7px;
        }

    

        .contact-item img {
            width: 30px;
            height: 30px;
        }

        .social-row {
            display: flex;
            align-items: center;
          
            margin-top: 10px;
            width: 100%;
        }

       .social-handle {
    font-size: 18px;
    margin-left: 15px;
    font-weight: 500;
}

        .social-icons-group {
            display: flex;
            gap: 10px;
        }

     .social-icon-circle {
    background: white;
    width: 30px;
    height: 30px;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
}
        .social-icon-circle img {
            width: 22px;
            height: 22px;
        }

        /* Helper for LTR text like phone numbers and emails */
        .ltr {
            direction: ltr;
            unicode-bidi: embed;
        }
        footer.store-footer a {
            color: #fff !important;
        }
        /* Responsive adjustments for smaller screens */
        @media (max-width: 860px) {
            .card {
                width: 95%;
                height: auto;
                flex-direction: column;
            }
            .left-section::after {
                display: none;
            }
            .right-section {
                align-items: center;
                padding: 40px 20px;
            }
            .contact-details {
                align-items: center;
            }
            .qr-area {
                align-self: center;
            }
        }
        .s-payments-list{
            padding:15px 0;
        }
    </style>


